      <li>
        <a role="menuitem" href="<%= APP_CONFIG['sv_base_url']%>forskolautbildning">Förskola &amp; utbildning</a>
      </li>
      <li>
        <a role="menuitem" href="<%= APP_CONFIG['sv_base_url']%>kommunpolitik">Kommun &amp; politik</a>
      </li>
      <li>
        <a role="menuitem" href="<%= APP_CONFIG['sv_base_url']%>kulturfritid">Kultur &amp; fritid</a>
